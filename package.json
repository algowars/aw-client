{
  "name": "aw-client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:coverage": "ng test --coverage",
    "test:ui": "vitest --ui",
    "lint": "ng lint",
    "format": "pnpm exec prettier --check .",
    "format:write": "pnpm exec prettier --write ."
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "pnpm@10.27.0",
  "engines": {
    "node": "24.x"
  },
  "dependencies": {
    "@angular-architects/ngrx-toolkit": "^21.0.1",
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/platform-browser": "^21.1.0",
    "@angular/router": "^21.1.0",
    "@ngrx/signals": "^21.0.1",
    "@primeuix/themes": "^2.0.3",
    "primeicons": "^7.0.0",
    "primeng": "^21.1.1",
    "rxjs": "~7.8.0",
    "tailwindcss-primeui": "^0.6.1",
    "tslib": "^2.3.0",
    "@ngrx/operators": "^21.0.1"
  },
  "devDependencies": {
    "@angular-eslint/builder": "21.2.0",
    "@angular/build": "^21.1.2",
    "@angular/cli": "^21.1.2",
    "@angular/compiler-cli": "^21.1.0",
    "@auth0/auth0-angular": "2.5.0",
    "@eslint/js": "^9.39.2",
    "@ngrx/operators": "^21.0.1",
    "@ngrx/signals": "^21.0.1",
    "@ngx-env/builder": "21.0.1",
    "@tailwindcss/postcss": "^4.1.12",
    "@vitest/coverage-v8": "4.0.18",
    "@vitest/ui": "4.0.18",
    "angular-eslint": "21.2.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "jsdom": "^27.1.0",
    "postcss": "^8.5.3",
    "prettier": "3.8.1",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.50.1",
    "vitest": "^4.0.8"
  }
}
